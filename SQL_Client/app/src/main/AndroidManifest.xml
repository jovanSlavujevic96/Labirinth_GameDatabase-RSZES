<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sql_client">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".tcp_socket_pkg.activites_pkg.UserSettingsActivity"></activity>
        <activity android:name=".xml_parser_pkg.activities_pkg.Leaderboard" />
        <activity android:name=".game_pkg.activities_pkg.GameActivity" />
        <activity android:name=".game_pkg.activities_pkg.GameMenu" />
        <activity android:name=".tcp_socket_pkg.activites_pkg.SignInActivity" />
        <activity android:name=".tcp_socket_pkg.activites_pkg.SignUpActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>